    "0.03 & 0.01 & 0.01 & 0.02 & 0.1 & 0.04 & 0.01 & 0.01 & 0.01 & 0.01 \\\\\n",
    "0.02 & 0.01 & 0.01 & 0.01 & 0.03 & 0.1 & 0.05 & 0.01 & 0.01 & 0.01 \\\\\n",
    "0.01 & 0.01 & 0.01 & 0.01 & 0.01 & 0.03 & 0.1 & 0.04 & 0.01 & 0.01 \\\\\n",
    "0.01 & 0.01 & 0.01 & 0.01 & 0.01 & 0.01 & 0.03 & 0.1 & 0.05 & 0.01 \\\\\n",
    "0.01 & 0.01 & 0.01 & 0.01 & 0.01 & 0.01 & 0.01 & 0.03 & 0.1 & 0.04 \\\\\n",
    "0.01 & 0.01 & 0.01 & 0.01 & 0.01 & 0.01 & 0.01 & 0.01 & 0.03 & 0.1\n",
    "\\end{pmatrix}\n",
    "$$\n",
    "\n",
    "$$\n",
