    "import numpy as np\n",
    "A = np.array([[1,1,1],[2,-1,1],[1,2,-1]]) # matriz de coeficientes\n",
    "b = np.array([6,3,4]) # vector de términos independientes, este puede ser de forma de fila o e columna\n",
    "sol = np.linalg.solve(A, b)\n",
    "print(sol)"
   ]
