    "$$\n",
    "A = \\begin{pmatrix}\n",
    "0.1 & 0.05 & 0.02 & 0.01 & 0.03 & 0.02 & 0.01 & 0.01 & 0.01 & 0.01 \\\\\n",
    "0.02 & 0.1 & 0.03 & 0.02 & 0.01 & 0.01 & 0.01 & 0.01 & 0.01 & 0.01 \\\\\n",
